{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress"
  ],
  "testRunner": "command",
  "commandRunner": {
    "command": "npm test"
  },
  "coverageAnalysis": "all",
  "mutate": ["src/**/*.js"],
  "ignorePatterns": ["node_modules/**/*", "test/**/*"],
  "tempDirName": ".stryker-tmp",
  "concurrency": 1,
  "cleanTempDir": true
}
