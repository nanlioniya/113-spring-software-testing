{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress"
  ],
  "testRunner": "mocha",
  "coverageAnalysis": "perTest",
  "mutate": ["src/**/*.js"],
  "mochaOptions": {
    "spec": ["test/calculator.mocha.js"]
  },
  "ignorePatterns": ["node_modules/**/*"],
  "tempDirName": ".stryker-tmp",
  "concurrency": 1,
  "cleanTempDir": true
}
